    "karma-phantomjs-launcher": "^1.0.2",
    "karma-typescript-preprocessor2": "^1.2.1",
    "karma-verbose-reporter": "0.0.3",
    "karma-webpack": "^1.8.1",
    "less": "^2.7.2",
    "less-loader": "^2.2.3",
    "lodash.debounce": "^4.0.8",
    "lodash.uniq": "^4.5.0",
