{
  "name": "mapguide-react-layout",
  "version": "0.0.1",
  "description": "An experimental react-based layout for MapGuide",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production webpack -p",
    "build:dev": "cross-env NODE_ENV=development webpack",
    "watch:dev": "cross-env NODE_ENV=development webpack -w",
    "clean": "rimraf dist/",
    "dev": "cross-env NODE_ENV=development webpack-dev-server -d --inline --hot --progress --no-info",
    "lint": "tslint 'src/**/*.tsx' 'src/**/*.ts' 'test/**/*.tsx' 'test/**/*.ts'",
    "postinstall": "npm run typings && npm run build",
    "typings": "rimraf typings/ && typings install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jumpinjackie/mapguide-react-layout.git"
  },
  "keywords": [
    "mapguide",
    "gis",
    "viewer"
  ],
  "author": "Jackie Ng",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jumpinjackie/mapguide-react-layout/issues"
  },
  "homepage": "https://github.com/jumpinjackie/mapguide-react-layout#readme",
  "devDependencies": {
    "@aneves/react-flyout": "^0.1.10",
    "cross-env": "^1.0.8",
    "css-loader": "^0.23.1",
    "es6-promise": "^3.2.1",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "helmet": "^2.1.1",
    "html-webpack-plugin": "^2.22.0",
    "http-proxy": "^1.14.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "lodash.debounce": "^4.0.6",
    "lodash.uniq": "^4.3.0",
    "node-uuid": "^1.4.7",
    "object-assign": "^4.1.0",
    "openlayers": "^3.17.1",
    "proj4": "^2.3.14",
    "raw-loader": "^0.5.1",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "rimraf": "^2.5.3",
    "style-loader": "^0.13.1",
    "ts-loader": "^0.8.2",
    "tslint": "^3.13.0",
    "tslint-loader": "^2.1.5",
    "typescript": "^1.8.10",
    "typings": "^1.3.1",
    "url-loader": "^0.5.7",
    "url-parse": "^1.1.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.12.1",
    "whatwg-fetch": "^1.0.0",
    "winston": "^2.2.0"
  }
}
