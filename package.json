{
  "name": "mapguide-react-layout",
  "version": "0.7.1",
  "description": "An experimental react-based layout for MapGuide",
  "scripts": {
    "prebuild": "yarn run clean",
    "build": "cross-env NODE_ENV=production webpack",
    "build:dev": "cross-env NODE_ENV=development webpack",
    "watch:dev": "cross-env NODE_ENV=development webpack -w --progress",
    "clean": "rimraf viewer/dist/",
    "dev": "cross-env NODE_ENV=development webpack-dev-server -d --inline --hot --progress --no-info",
    "lint": "tslint 'src/**/*.tsx' 'src/**/*.ts' 'test/**/*.tsx' 'test/**/*.ts'",
    "postinstall": "yarn run typings && yarn run build",
    "typings": "rimraf typings/ && typings install",
    "test": "karma start",
    "test:ci": "karma start --single-run --browsers PhantomJS --reporters=verbose",
    "ci": "yarn run typings && yarn run lint && yarn run test:ci"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jumpinjackie/mapguide-react-layout.git"
  },
  "keywords": [
    "mapguide",
    "gis",
    "viewer"
  ],
  "author": "Jackie Ng",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jumpinjackie/mapguide-react-layout/issues"
  },
  "homepage": "https://github.com/jumpinjackie/mapguide-react-layout#readme",
  "devDependencies": {
    "@blueprintjs/core": "^1.5.0",
    "@types/chai": "3.4.34",
    "@types/enzyme": "2.7.1",
    "@types/lodash.debounce": "4.0.0",
    "@types/lodash.uniq": "4.5.0",
    "@types/lodash.xor": "^4.5.0",
    "@types/mocha": "2.2.36",
    "@types/node": "6.0.58",
    "@types/query-string": "3.0.31",
    "@types/react": "0.14.55",
    "@types/react-addons-css-transition-group": "0.14.19",
    "@types/react-addons-update": "0.14.15",
    "@types/react-dom": "0.14.20",
    "@types/react-measure": "0.4.5",
    "@types/shortid": "0.0.28",
    "chai": "^3.5.0",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "enzyme": "^2.7.0",
    "es6-promise": "^4.0.5",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
