{
  "name": "mapguide-react-layout",
  "version": "0.4.0",
  "description": "An experimental react-based layout for MapGuide",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production webpack",
    "build:dev": "cross-env NODE_ENV=development webpack",
    "watch:dev": "cross-env NODE_ENV=development webpack -w --progress",
    "clean": "rimraf viewer/dist/",
    "dev": "cross-env NODE_ENV=development webpack-dev-server -d --inline --hot --progress --no-info",
    "lint": "tslint 'src/**/*.tsx' 'src/**/*.ts' 'test/**/*.tsx' 'test/**/*.ts'",
    "postinstall": "npm run typings && npm run build",
    "typings": "rimraf typings/ && typings install",
    "test": "karma start",
    "test:ci": "karma start --single-run --browsers PhantomJS --reporters=verbose",
    "ci": "npm run typings && npm run lint && npm run test:ci"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jumpinjackie/mapguide-react-layout.git"
  },
  "keywords": [
    "mapguide",
    "gis",
    "viewer"
  ],
  "author": "Jackie Ng",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jumpinjackie/mapguide-react-layout/issues"
  },
  "homepage": "https://github.com/jumpinjackie/mapguide-react-layout#readme",
  "devDependencies": {
    "@aneves/react-flyout": "^0.1.11",
    "chai": "^3.5.0",
    "cross-env": "^1.0.8",
    "css-loader": "^0.23.1",
    "enzyme": "^2.4.1",
    "es6-promise": "^4.0.5",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "helmet": "^2.3.0",
    "html-webpack-plugin": "^2.22.0",
    "http-proxy": "^1.15.1",
    "ismobilejs": "^0.4.0",
    "karma": "^1.3.0",
    "karma-chai": "^0.1.0",
    "karma-mocha": "^1.2.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-typescript-preprocessor2": "^1.2.1",
    "karma-verbose-reporter": "0.0.3",
    "karma-webpack": "^1.8.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "lodash.debounce": "^4.0.8",
    "lodash.uniq": "^4.5.0",
    "mocha": "^3.1.2",
    "node-uuid": "^1.4.7",
    "object-assign": "^4.1.0",
    "openlayers": "3.18.2",
    "phantomjs-prebuilt": "^2.1.13",
    "proj4": "^2.3.15",
    "query-string": "^4.2.3",
    "raw-loader": "^0.5.1",
    "react": "^15.3.2",
    "react-addons-test-utils": "^15.3.2",
    "react-addons-update": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-draggable": "^2.2.2",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0",
    "redux-localstorage": "^0.4.1",
    "redux-logger": "^2.7.0",
    "redux-thunk": "^2.1.0",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "testdouble": "^1.8.0",
    "ts-loader": "^0.9.3",
    "tslint": "^3.15.1",
    "tslint-loader": "^2.1.5",
    "typescript": "^2.0.3",
    "typings": "^1.4.0",
    "url-loader": "^0.5.7",
    "url-parse": "^1.1.6",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2",
    "webpack-hot-middleware": "^2.13.0",
    "whatwg-fetch": "^1.0.0",
    "winston": "^2.2.0"
  }
}
